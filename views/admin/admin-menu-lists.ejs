<%- include admin-header %>
	<table class="table table-condensed">
		<thead>
			<tr>
				<th>name</th>
				<th>picture</th>
				<th>sub name</th>
				<th>sales</th>
				<th>prev price</th>
				<th>now price</th>
				<th></th>
				<th></th>
			</tr>
		</thead>
		<tbody>
		<% data.forEach(function(item,i){ %>
			<tr>
				<td><a href=""><%= item.name %></a></td>
				<td><img width=100 height=100 src="/<%= item.img_src %>" alt=""/></td>
				<td><%= item.sub_name %></td>
				<td><%= item.sale_count %></td>
				<td><%= item.prev_price %></td>
				<td><%= item.now_price %></td>
				<td><a href="/admin/menu_lists?menu-id=<%= item._id %>&&action=delete">删除</a></td>
				<td><a href="/admin/menu_lists?menu-id=<%= item._id %>&&action=edit">修改</a></td>
			</tr>
		<% }) %>
		</tbody>
	</table>
<%- include admin-footer %>